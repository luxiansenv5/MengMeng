<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mengmeng.activity">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITEEXTERNALSTORAGE" />

    <application
        android:name="application.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".DynamicMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DynamicInfoActivity" />
        <activity android:name=".UserInfoActivity" />
        <activity android:name=".ReleaseActivity" />
        <activity
            android:name="com.example.mengmeng.serviceactivity.AdoptActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name="com.example.mengmeng.serviceactivity.SelectPet"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity android:name=".CommunicateGroupActivity" />
        <activity android:name=".CommunicatePetFriendAdd" />
        <activity android:name=".CommunicatePetFriendSearch" />
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".PersonDataActivity"></activity>
    </application>

</manifest>